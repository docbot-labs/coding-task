{
  "name": "@docbot/backend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed": "prisma db seed",
    "generate": "prisma generate",
    "migrate": "prisma migrate dev",
    "script": "node -r esbuild-register",
    "proto:generate": "proto-loader-gen-types --longs=String --enums=String --defaults --oneofs --grpcLib=@grpc/grpc-js --outDir=proto/ ../proto/*.proto"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.6.7",
    "@grpc/proto-loader": "^0.6.12",
    "@nestjs/apollo": "^10.0.8",
    "@nestjs/axios": "^0.0.7",
    "@nestjs/bullmq": "^0.0.4",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/graphql": "^10.0.8",
    "@nestjs/microservices": "^8.4.6",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.0.0",
    "@octokit/auth-app": "^3.6.1",
    "@octokit/auth-token": "^2.5.0",
    "@octokit/request-error": "^2.1.0",
    "@prisma/client": "^3.11.1",
    "@sanity/client": "^3.3.0",
    "@slack/bolt": "^3.11.0",
    "algoliasearch": "^4.13.0",
    "apollo-server-express": "^3.6.7",
    "bullmq": "^1.80.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "date-fns": "^2.28.0",
    "figma-api": "^1.10.1",
    "graphql": "^16.3.0",
    "gray-matter": "^4.0.3",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.5",
    "mdurl": "^1.0.1",
    "node-fetch": "^2.6.7",
    "octokit": "^1.7.1",
    "openid-client": "^5.1.6",
    "passport": "^0.5.2",
    "passport-headerapikey": "^1.2.2",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "remark-html": "^13.0.1",
    "remark-parse": "^9.0.0",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "slug": "^5.3.0",
    "tiny-invariant": "^1.2.0",
    "unified": "^9.2.2",
    "winston": "^3.7.2",
    "yaml": "^2.0.1",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@octokit/webhooks-types": "^5.5.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "esbuild": "^0.14.42",
    "esbuild-register": "^3.3.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "prisma": "^3.11.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  }
}
